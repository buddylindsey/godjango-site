<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>{% block title %}Django Screencasts and Tutorials - GoDjango.com{% endblock %}</title>
    <meta name="description" content="{% block metadescription %}Learn something new about Django you didn't know before with short easy to understand screencasts and tutorials.{% endblock %}">
    <meta name="keywords" content="{% block metakeywords %}django screencasts, django, django videos, django weekly, django tutorials{% endblock %}">
    <meta http-equiv="content-language" content="en">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="//cdn.optimizely.com/js/2292090798.js"></script>
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='//fonts.googleapis.com/css?family=Karla:400,400italic,700,700italic' rel='stylesheet' type='text/css'>
    {{ compressed_css('vendor') }}
    {{ compressed_css('main') }}
    {% block head %}{% endblock %}
    <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-805005-16']);
      _gaq.push(['_trackPageview']);
      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'stats.g.doubleclick.net/dc.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
    </script>
  </head>
  <body {% block body_extra %}{% endblock %}>
    <nav class="navbar navbar-inverse no-border-radius{% if index %} margin-bottom-0{% endif %}" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#auth-navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="{{ url("index") }}"><img src="{{ static("img/godjango-logo-sm.png") }}" /></a>
            </div>
            <div class="collapse navbar-collapse" id="auth-navbar-collapse">
                <ul class="nav navbar-nav">
                    {% set customer, paying = is_customer(user) %}
                    {% if not customer or not paying %}
                    <li class="go-pro-li"><a href="{{ url("subscribe") }}">GoDjango <strong>Pro</strong> subscription</a></li>
                    {% endif %}
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Videos <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="{{ url("browse") }}">All</a></li>
                            <li><a href="{{ url("category", "basics") }}">Basics</a></li>
                            <li><a href="{{ url("category", "advanced") }}">Advanced</a></li>
                            <li><a href="{{ url("category", "3rd-party-apps") }}">3rd Party Apps</a></li>
                            <li><a href="{{ url("category", "class-based-views") }}">Class Based Views</a></li>
                            <li><a href="{{ url("category", "assets") }}">Assets</a></li>
                            <li><a href="{{ url("category", "devops") }}">DevOps</a></li>
                            <li><a href="{{ url("category", "testing") }}">Testing</a></li>
                            <li><a href="{{ url("category", "templates") }}">Templates</a></li>
                            <li class="divider"></li>
                            <li><a href="{{ url("series", "beginning-django") }}">Beginning Django</a></li>
                            <li><a href="{{ url("series", "building-a-product") }}">Building a Product</a></li>
                            <li><a href="{{ url("series", "django-and-angular-basics") }}">Django and Angular Basics</a></li>
                            <li><a href="{{ url("series", "reactjs-redux-and-django") }}">React + Redux and Django</a></li>
                            <li class="divider"></li>
                            <li><a href="{{ url("category", "pro") }}">Pro</a></li>
                        </ul>
                    </li>
                    <li><a href="{{ url("podcast") }}" class="main-link">Podcast</a></li>
                    <li><a href="{{ url("djblog:index") }}" class="main-link">Blog</a></li>
                    <li><a href="{{ url("feedback") }}" class="main-link">Feedback</a></li>
                    <li><a href="{{ url("about") }}" class="main-link">About</a></li>
                </ul>
                <form class="navbar-form navbar-right" method="get" action="{{ url('search') }}" role="search">
                  <div class="form-group">
                    <input type="text" class="form-control" placeholder="Search" name="q" />
                  </div>
                </form>
                <ul class="nav navbar-nav navbar-right">
                  {% if user.is_authenticated() %}
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">{{ user.username }} <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="{{ url("dashboard") }}">dashboard</a></li>
                            <li><a href="{{ url("billing") }}">subscription</a></li>
                            <li class="divider"></li>
                            <li><a href="{{ url("settings") }}">change password</a></li>
                            <li class="divider"></li>
                            <li><a href="{{ url("logout") }}"><i class="fa fa-sign-out"></i>&nbsp;logout</a></li>
                            {% if user.is_superuser %}
                            <li class="divider"></li>
                            <li><a href="{{ url('analytics') }}"><i class="fa fa-area-chart"></i>&nbsp;analytics</a></li>
                            <li><a href="/admin/"><i class="fa fa-rebel"></i>&nbsp;admin</a></li>
                            {% endif %}
                        </ul>
                    </li>
                  </div>
                  {% else %}
                  <li><a href="{{ url("register") }}" class="main-link">create free account</a></li>
                  <li><a href="{{ url("login") }}" class="main-link"><i class="fa fa-sign-in"></i>&nbsp;login</a>
                  {% endif %}
                </ul>
            </div>
        </div>
    </nav>
    <div class="{%- block container -%}container{%- endblock -%}">
    {%- block content -%}
    {%- endblock -%}
    </div>
    <footer>
      <div class="continer-fluid">
        <div class="row">
          <div class="col-sm-12 text-center">
            <p>
            &copy; 2011-2016 GoDjango
            </p>
            <a href="{{ url("privacy") }}">Privacy</a>
            <a href="{{ url("feedback") }}">Contact</a>
          </div>
        </div>
      </div>
    </footer>
    <script type="text/javascript" charset="utf-8" src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
    <script type="text/javascript" charset="utf-8" src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
    <script type="text/javascript" charset="utf-8" src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.0/angular.min.js"></script>
    {{ compressed_js('vendor') }}
    {{ compressed_js('main') }}
    {%- block additional_js -%}
    {%- endblock -%}
    <!-- Drip -->
    <script type="text/javascript">
      var _dcq = _dcq || [];
      var _dcs = _dcs || {}; 
      _dcs.account = '5615415';
  
      (function() {
        var dc = document.createElement('script');
        dc.type = 'text/javascript'; dc.async = true; 
        dc.src = '//tag.getdrip.com/5615415.js';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(dc, s);
      })();
    </script>
  </body>
</html>
