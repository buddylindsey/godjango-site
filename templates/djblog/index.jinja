{% extends "base.jinja" %}

{% block title %}
GoDjango Blog
{% endblock %}

{% block content %}
<div class="row">
  <div class="col-sm-12">
  {% for article in articles %}
  <h2><a href="{{ article.get_absolute_url() }}">{{ article.title }}</a></h2>
  {{ article.published_body()|safe }}
  <hr />
  {% endfor %}
  </div>
</div>
{% endblock %}
