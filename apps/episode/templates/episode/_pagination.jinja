        <ul class="pagination">
            {% if page_obj.has_previous() %}
            <li><a href="?page={{ page_obj.previous_page_number() }}{% if query %}&q={{ query }}{% endif %}">Previous</a></li>
            {% endif %}
            {% for pg in paginator.page_range %}
            {% if page_obj.number == pg %}
            <li class="active"><a href="?page={{ pg }}{% if query %}&q={{ query }}{% endif %}">{{ pg }}</a></li>
            {% else %}
            <li><a href="?page={{ pg }}{% if query %}&q={{ query }}{% endif %}">{{ pg }}</a></li>
            {% endif %}
            {% endfor %}
            {% if page_obj.has_next() %}
            <li><a href="?page={{ page_obj.next_page_number() }}{% if query %}&q={{ query }}{% endif %}">Next</a></li>
            {% endif %}
        </ul>
